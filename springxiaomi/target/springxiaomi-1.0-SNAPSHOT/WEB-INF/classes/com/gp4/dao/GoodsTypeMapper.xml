<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.gp4.dao.GoodsTypeMapper">

    <select id="findByLevel" resultType="com.gp4.pojo.GoodsType">
        select * from tb_goods_type where level=#{level}
    </select>

    <select id="findById" resultType="com.gp4.pojo.GoodsType">
        select * from tb_goods_type where id=#{id}
    </select>
</mapper>